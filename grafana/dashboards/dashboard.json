  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "panels": [
    {
      "type": "table",
      "title": "Requisições por Status de Resposta (Diário) - Total de Requisições por Status",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": true
          }
        ]
      },
      "targets": [
        {
          "expr": "sum by (status)(increase(http_requests_received_total{job=\"api\"}[1d]))",
          "format": "table",
          "instant": false,
          "intervalFactor": 2,
          "legendFormat": "Status {{status}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "Latência por Endpoint",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "showPoints": "auto",
            "pointSize": 5,
            "stacking": {
              "mode": "none"
            },
            "spanNulls": false
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "calcs": []
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"api\"}[5m])) by (le, endpoint))",
          "format": "time_series",
          "instant": false,
          "intervalFactor": 2,
          "legendFormat": "Endpoint {{endpoint}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 9
      }
    },
